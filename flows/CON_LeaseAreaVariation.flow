    <decisions>
        <description>Checks if entered Effective Date is between Lease Start Date and Lease End Date</description>
        <name>isEffectiveDateValid</name>
        <label>isEffectiveDateValid</label>
        <locationX>378</locationX>
        <locationY>404</locationY>
        <defaultConnector>
            <targetReference>Invalid_Effective_Date</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>ValidEffectiveDate</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varLeaseStartDate</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Effective_Date</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varLeaseEndDate</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Effective_Date</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getLeaseAreaVariationRecType</targetReference>
            </connector>
            <label>ValidEffectiveDate</label>
        </rules>
    </decisions>
    <decisions>
        <name>isSuccess</name>
        <label>isSuccess</label>
        <locationX>600</locationX>
        <locationY>284</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>SuccessfullyCreated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varNewCreatedCaseId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>NavigateToCase</targetReference>
            </connector>
            <label>SuccessfullyCreated</label>
        </rules>
        <rules>
            <name>CaseCreationFailed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varNewCreatedCaseId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CreateFailed</targetReference>
            </connector>
            <label>CaseCreationFailed</label>
        </rules>
    </decisions>
