    <validationRules>
        <fullName>PRC_CheckBlanketBGDetailsAreEntered</fullName>
        <active>true</active>
        <description>If the security deposit type is Blanket Bank Guarantee then Blanket BG details should be entered</description>
        <errorConditionFormula>AND(ISPICKVAL(SecurityDepositType__c,&apos;Blanket Bank Guarantee&apos;),ISBLANK(BlanketBGDetails__c))</errorConditionFormula>
        <errorMessage>Please enter the Blanket BG Details for the security deposit type &quot;Blanket Bank Guarantee&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_CheckMinandMaxThreshold</fullName>
        <active>true</active>
        <description>Current Minimum Threshold should be less than the Current Maximum Threshold.</description>
        <errorConditionFormula>CurrentMaximumThreshold__c &lt;= CurrentMinimumThreshold__c</errorConditionFormula>
        <errorMessage>Current Minimum Threshold should be less than the Current Maximum Threshold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_CheckSeurityDepositWeeksMonthsYears</fullName>
        <active>true</active>
        <description>User can only enter only one of the values Required Security Deposit (Days), Required Security Deposit (Months), Required Security Deposit (Years)</description>
        <errorConditionFormula>AND(ISPICKVAL(SecurityDepositRequired__c,&apos;Yes&apos;),
OR(
AND(ISBLANK(RequiredSecurityDepositDays__c),ISBLANK(RequiredSecurityDepositMonths__c),ISBLANK(RequiredSecurityDepositWeeks__c)),
OR(
AND(NOT(ISBLANK(RequiredSecurityDepositWeeks__c)),NOT(ISBLANK(RequiredSecurityDepositMonths__c))),
AND(NOT(ISBLANK(RequiredSecurityDepositWeeks__c)),NOT(ISBLANK(RequiredSecurityDepositDays__c))),
AND(NOT(ISBLANK(RequiredSecurityDepositDays__c)),NOT(ISBLANK(RequiredSecurityDepositMonths__c))),
AND(NOT(ISBLANK(RequiredSecurityDepositDays__c)),NOT(ISBLANK(RequiredSecurityDepositMonths__c)),NOT(ISBLANK(RequiredSecurityDepositWeeks__c)))
)
)
)</errorConditionFormula>
        <errorMessage>1) Only one value can be entered in either Required Security Deposit (Weeks),Required 
    Security Deposit (Months) or Required Security Deposit (Days)
2) All Required Security Deposit fields can not be BLANK</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_CurrentMinMaxThreshold</fullName>
        <active>false</active>
        <description>If user enters the value in Current Minimum Threshold, then Current Maximum Threshold should be entered and it’s applicable other way round as well.</description>
        <errorConditionFormula>OR(AND( ISBLANK( CurrentMaximumThreshold__c ), NOT(ISBLANK( CurrentMinimumThreshold__c ))), AND( ISBLANK( CurrentMinimumThreshold__c ), NOT(ISBLANK( CurrentMaximumThreshold__c ))))</errorConditionFormula>
        <errorMessage>Both Current Minimum Threshold and Current Maximum Threshold should be entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_PromoPercentageOnNationalAgreement</fullName>
        <active>false</active>
        <description>“It is mandatory for the user to enter the Promo Percentage on the National Agreement, if any of the field  Current Maximum Threshold &amp; Current Minimum Threshold is not blank</description>
        <errorConditionFormula>AND(OR(NOT( ISBLANK( CurrentMaximumThreshold__c ) ) , NOT( ISBLANK(  CurrentMinimumThreshold__c ) )),  ISBLANK( PromoPercentage__c ) )</errorConditionFormula>
        <errorMessage>Promo Percentage needs to be populated if Current Maximum Threshold or Current Minimum Threshold is not blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_RecordCreateRestriction</fullName>
        <active>true</active>
        <description>National agreement can be only created by to Retail admin team</description>
        <errorConditionFormula>AND( 
AND( 
($Profile.Name != &quot;SCG-Retail Admin&quot;), 
($Profile.Name != &quot;System Administrator&quot;), 
($Profile.Name != &quot;SCG-SysAdm&quot;), 
($Profile.Name != &quot;SCG-Sys DataMigration&quot;)),  
ISNEW(),
OR(
ISPICKVAL(Account__r.PartnershipSegmentation__c,&quot;Strategic Partner&quot;),
ISPICKVAL(Account__r.PartnershipSegmentation__c,&quot;Growth Partner&quot;) 
)
)</errorConditionFormula>
        <errorMessage>Only Retail admin team can create the national agreement or precedent clauses for Strategic Partner or Growth Partner Accounts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRC_RecordUpdateRestriction</fullName>
        <active>true</active>
        <description>National agreement can be only changed to Retail admin team</description>
        <errorConditionFormula>AND( 
AND( 
($Profile.Name != &quot;SCG-Retail Admin&quot;), 
($Profile.Name != &quot;System Administrator&quot;), 
($Profile.Name != &quot;SCG-SysAdm&quot;), 
($Profile.Name != &quot;SCG-Sys DataMigration&quot;)),  
NOT(ISNEW()),
OR(
ISPICKVAL(Account__r.PartnershipSegmentation__c,&quot;Strategic Partner&quot;),
ISPICKVAL(Account__r.PartnershipSegmentation__c,&quot;Growth Partner&quot;) 
)
)</errorConditionFormula>
        <errorMessage>Only Retail admin team can change the national agreement or precedent clauses for Strategic Partner or Growth Partner Accounts</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
