    <validationRules>
        <fullName>OPC_LockClauseDetailWhenSpecialType</fullName>
        <active>false</active>
        <description>stop the user to edit the Clause Details when the type is Special Condition.</description>
        <errorConditionFormula>AND(ISCHANGED( ClauseDetails__c ),  
OR(TEXT( Type__c ) = &apos;Special Condition&apos;,TEXT( Type__c ) = &apos;RCA&apos;),
 $Profile.Name = &apos;SCG-Retail Lease&apos;)</errorConditionFormula>
        <errorMessage>Can not edit the clause details because type is special condition</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPC_LockFieldsonDealAchieved</fullName>
        <active>true</active>
        <description>The system must prevent Users from modifying key details on the Opportunity Record once the deal is achieved</description>
        <errorConditionFormula>AND(Opportunity__r.DealAchieved__c,$Profile.Name &lt;&gt; &quot;SCG-Sys DataMigration&quot;,$Profile.Name &lt;&gt; &quot;SCG-Sys Integration&quot;,
ISPICKVAL(Type__c,&quot;Usage&quot;),
ISCHANGED(ClauseDetails__c))</errorConditionFormula>
        <errorMessage>You cannot modify a number of key fields within the Opportunity Record once the deal has been achieved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPC_RequiredReasonRecommendation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	NOT(IsAutomated__c),
	ReferencedClause__r.HighRiskClause__c,
	OR(
		ReasonRecommendation__c == ReferencedClause__r.ReasonRecommendation__c,
		ISNULL(ReasonRecommendation__c),
		ISBLANK(ReasonRecommendation__c)
	)
)</errorConditionFormula>
        <errorDisplayField>ReasonRecommendation__c</errorDisplayField>
        <errorMessage>The following condition/s require risk approval, please provide your recommendation/input for approval before submitting your request</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
