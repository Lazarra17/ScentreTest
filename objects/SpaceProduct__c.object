    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>&lt;SPP&gt; - Represents a products delated to the Space where Space product values is different to property. This caters for space level outgoings eg. Council rates, Air Con</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>Active__c</fullName>
        <description>Confirms that the Space Product can currently be used. This field is driven by the effective dates defined for this space product.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(EffectiveToDate__c), 
IF(EffectiveFromDate__c &lt;= TODAY(),TRUE,FALSE), 
(AND(EffectiveFromDate__c &lt;= TODAY(), 
TODAY() &lt;= EffectiveToDate__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Confirms that the Space Product can currently be used. This field is driven by the effective dates defined for this space product.</inlineHelpText>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CalculationMethod__c</fullName>
        <description>Drives the price calculation.</description>
        <externalId>false</externalId>
        <label>Calculation Method</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Fixed</fullName>
                    <default>false</default>
                    <label>Fixed</label>
                </value>
                <value>
                    <fullName>Percentage</fullName>
                    <default>false</default>
                    <label>Percentage</label>
                </value>
                <value>
                    <fullName>PSM</fullName>
                    <default>false</default>
                    <label>PSM</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>Describes the product / service.</description>
        <externalId>false</externalId>
        <label>Description</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>EffectiveFromDate__c</fullName>
        <description>The Date from which this Product becomes or became available for use for any Opportunity related to the Space.</description>
        <externalId>false</externalId>
        <label>Effective From Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>EffectiveToDate__c</fullName>
        <description>The Date until which this Product was or will remain available for use for any Opportunity related to the Space.</description>
        <externalId>false</externalId>
        <label>Effective To Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Percentage__c</fullName>
        <description>Store the applicable percentage value if the price is calculated based on a percentage (based on the selected &quot;Calculation Method&quot;)</description>
        <externalId>false</externalId>
        <inlineHelpText>Provide a percentage if the calculation method is percentage.</inlineHelpText>
        <label>Percentage</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>ProductFamily__c</fullName>
        <description>The selected Product&apos;s Family. Note: This field has been created as a picklist rather than a formula, to allow using roll-up summary fields in the future.</description>
        <externalId>false</externalId>
        <inlineHelpText>The selected Product&apos;s Family.</inlineHelpText>
        <label>Product Family</label>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>ProductFamily</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>Product__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>The Product used for this Space Product.</description>
        <externalId>false</externalId>
        <label>Product</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>You can select only recoveries product.</errorMessage>
            <filterItems>
                <field>Product2.BillCode__c</field>
                <operation>notEqual</operation>
                <value></value>
            </filterItems>
            <filterItems>
                <field>Product2.Family</field>
                <operation>equals</operation>
                <value>Recoveries</value>
            </filterItems>
            <filterItems>
                <field>Product2.EffectiveFromDate__c</field>
                <operation>notEqual</operation>
                <value>1/1/2018</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>Space Products</relationshipLabel>
        <relationshipName>Space_Products</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Property__c</fullName>
        <description>The Property to which this Space Product applies. This field is defaulted based on the selected Space.</description>
        <externalId>false</externalId>
        <formula>Space__r.Property__r.Name</formula>
        <label>Property</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SalesPrice__c</fullName>
        <description>The Unit Price applying to the Product for this Space. The Sales Price is applied based on the Calculation Method and does not apply for Percentage calculations.</description>
        <externalId>false</externalId>
        <inlineHelpText>Provide a dollar ($) psm amount if calculation method is PSM.</inlineHelpText>
        <label>Sales Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Space__c</fullName>
        <description>The Space to which the Space Product applies.</description>
        <externalId>false</externalId>
        <label>Space</label>
        <referenceTo>Space__c</referenceTo>
        <relationshipLabel>Space Products</relationshipLabel>
        <relationshipName>SpaceProducts</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
