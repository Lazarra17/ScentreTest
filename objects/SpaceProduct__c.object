    <validationRules>
        <fullName>SP_PercentageCheck</fullName>
        <active>true</active>
        <description>Percentage value Cannot be empty for Percentage calculation method.</description>
        <errorConditionFormula>AND(ISPICKVAL(CalculationMethod__c,&apos;Percentage&apos;),ISBLANK(Percentage__c))</errorConditionFormula>
        <errorDisplayField>Percentage__c</errorDisplayField>
        <errorMessage>Percentage value Cannot be empty for Percentage calculation method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SP_SalesPriceCheck</fullName>
        <active>true</active>
        <description>Salesprice Cannot be empty for PSM and Fixed calculation method.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(CalculationMethod__c,&apos;PSM&apos;),ISPICKVAL(CalculationMethod__c,&apos;Fixed&apos;)),ISBLANK(SalesPrice__c))</errorConditionFormula>
        <errorDisplayField>SalesPrice__c</errorDisplayField>
        <errorMessage>Sales Price Cannot be empty for PSM and Fixed calculation method.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
